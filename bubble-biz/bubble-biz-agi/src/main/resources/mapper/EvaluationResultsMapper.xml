<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bubblecloud.biz.agi.mapper.EvaluationResultsMapper">

  <resultMap id="evaluationResultsMap" type="com.bubblecloud.agi.api.entity.EvaluationResults">
        <id property="id" column="id"/>
        <result property="taskId" column="task_id"/>
        <result property="dbId" column="db_id"/>
        <result property="benchmarkId" column="benchmark_id"/>
        <result property="status" column="status"/>
        <result property="retrievalConfig" column="retrieval_config"/>
        <result property="metrics" column="metrics"/>
        <result property="overallScore" column="overall_score"/>
        <result property="totalQuestions" column="total_questions"/>
        <result property="completedQuestions" column="completed_questions"/>
        <result property="completedAt" column="completed_at"/>
        <result property="createdAt" column="created_at"/>
        <result property="createdBy" column="created_by"/>
  </resultMap>
  
  <select id="findPg" resultType="com.bubblecloud.agi.api.entity.EvaluationResults">
     SELECT t.*
     FROM evaluation_results t
  </select>
	
</mapper>