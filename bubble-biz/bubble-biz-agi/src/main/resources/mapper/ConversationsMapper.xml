<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bubblecloud.biz.agi.mapper.ConversationsMapper">

  <resultMap id="conversationsMap" type="com.bubblecloud.agi.api.entity.Conversations">
        <id property="id" column="id"/>
        <result property="threadId" column="thread_id"/>
        <result property="userId" column="user_id"/>
        <result property="agentId" column="agent_id"/>
        <result property="title" column="title"/>
        <result property="status" column="status"/>
        <result property="createdAt" column="created_at"/>
        <result property="updatedAt" column="updated_at"/>
        <result property="extMetadata" column="ext_metadata"/>
  </resultMap>
  
  <select id="findPg" resultType="com.bubblecloud.agi.api.entity.Conversations">
     SELECT t.*
     FROM conversations t
  </select>
	
</mapper>