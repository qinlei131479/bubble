<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bubblecloud.biz.agi.mapper.DatasourceTableFieldMapper">

  <resultMap id="datasourceTableFieldMap" type="com.bubblecloud.agi.api.entity.DatasourceTableField">
        <id property="id" column="id"/>
        <result property="dsId" column="ds_id"/>
        <result property="tableId" column="table_id"/>
        <result property="fieldName" column="field_name"/>
        <result property="fieldType" column="field_type"/>
        <result property="fieldComment" column="field_comment"/>
        <result property="customComment" column="custom_comment"/>
        <result property="weight" column="weight"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
  </resultMap>
  
  <select id="findPg" resultType="com.bubblecloud.agi.api.entity.DatasourceTableField">
     SELECT t.*
     FROM datasource_table_field t
  </select>
	
</mapper>